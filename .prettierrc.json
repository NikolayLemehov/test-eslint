{
  "semi": true,
  "tabWidth": 2,
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "es5",
  "jsxSingleQuote": false,
  "bracketSpacing": true,
  "bracketSameLine": true,
  "arrowParens": "always",
  "overrides": [
    {
      "files": ["**/*.css", "**/*.scss"],
      "options": {
        "tabWidth": 2,
        "singleQuote": false,
        "bracketSameLine": true,
        "bracketSpacing": true
      }
    }
  ]
}
